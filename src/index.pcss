.App {
  font-family: Arial, sans-serif;
  padding: 20px;
  text-align: center;
}

input {
  padding: 10px;
  width: 300px;
  margin-right: 10px;
  border-radius: 5px;
  border: 1px solid #ccc;
}

button {
  padding: 10px 15px;
  background-color: #ffeb3b;
  border: none;
  border-radius: 5px;
  cursor: pointer;
}

button:hover {
  background-color: #fdd835;
}

.post-it-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 20px;
}

.post-it {
  background-color: #ffeb3b;
  border: 1px solid #fdd835;
  padding: 20px;
  margin: 10px;
  width: 200px;
  text-align: center;
  box-shadow: 2px 2px 8px rgba(0, 0, 0, 0.2);
  position: relative;
}

.post-it button {
  position: absolute;
  top: 10px;
  right: 10px;
  background-color: #f44336;
  color: white;
  border: none;
  border-radius: 50%;
  width: 25px;
  height: 25px;
  line-height: 15px;
  cursor: pointer;
}

.post-it button:hover {
